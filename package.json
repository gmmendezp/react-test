{
  "name": "react-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean": "rimraf build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "standard --parser babel-eslint --verbose | snazzy",
    "format": "prettier --single-quote --no-semi --write \"**/*.js\" && standard --fix",
    "precommit": "lint-staged"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --no-semi --write",
      "standard --fix",
      "git add"
    ]
  },
  "dependencies": {
    "csstips": "^0.2.0",
    "csx": "^8.2.1",
    "react": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-form": "^7.0.4",
    "redux-observable": "^0.16.0",
    "typestyle": "^1.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "enzyme": "^2.8.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "prettier": "^1.4.2",
    "react-dom": "^15.5.4",
    "react-scripts": "1.0.13",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
  }
}
